http-response ^https?:\/\/.+\.(bilibili|biliapi)\.(com|net)\/x\/v2\/account\/myinfo\?access_key requires-body=1,max-size=0,script-path=https://hub.nange.cn/Script/Bilibili/MyInfo.js
